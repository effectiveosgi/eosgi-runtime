version: 2.2.0

Provide-Capability: \
	osgi.enroute.webresource;\
		osgi.enroute.webresource="/google/polymer";\
		version:Version=${version};\
		root="/bower_components"

Require-Capability: \
    osgi.implementation; \
        filter:='(&(osgi.implementation=osgi.enroute.webresource)(version>=1.0.0)(!(version>=2.0.0)))'