-standalone: target/index.xml

-runsystempackages: sun.reflect
-runfw: org.apache.felix.framework
-runee: JavaSE-1.8
-resolve.effective: active; skip:='osgi.service'

-runrequires: \
        osgi.identity;filter:='(osgi.identity=com.effectiveosgi.rt.config)',\
        osgi.identity;filter:='(osgi.identity=com.effectiveosgi.rt.config.test)'

-runbundles: \
	com.effectiveosgi.rt.config.test;version='[0.0.1,0.0.2)',\
	org.apache.felix.configadmin;version='[1.8.14,1.8.15)',\
	org.apache.felix.fileinstall;version='[3.5.4,3.5.5)',\
	org.apache.felix.log;version='[1.0.1,1.0.2)',\
	org.apache.felix.scr;version='[2.0.10,2.0.11)',\
	org.apache.servicemix.bundles.junit;version='[4.12.0,4.12.1)',\
	net.bytebuddy.byte-buddy;version='[1.7.4,1.7.5)',\
	net.bytebuddy.byte-buddy-agent;version='[1.7.4,1.7.5)',\
	org.mockito.mockito-core;version='[2.11.0,2.11.1)',\
	org.objenesis;version='[2.6.0,2.6.1)',\
	com.effectiveosgi.rt.config;version='[0.1.0,0.1.1)'
